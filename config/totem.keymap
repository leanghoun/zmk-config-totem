        mac1: mac1 {
            label = "mac1";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_tap &kp L &kp E &kp A &kp N &kp G &kp H &kp O &kp U &kp N &kp AT &kp G &kp M &kp A &kp I &kp L &kp DOT &kp C &kp O &kp M>
                ;
        }; mac2: mac2 {
            label = "mac2";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_tap &kp H &kp A &kp N &kp N &kp A &kp H &kp LPAR &kp N2 &kp N8 &kp RPAR>
                , <&macro_press &kp LSHFT>
                , <&macro_tap &kp M>
                , <&macro_release &kp LSHFT>
                , <&macro_tap &kp A &kp Y>
                ;
        };
